{
    "name": "exsyst/io",
    "type": "library",
    "description": "Object-oriented I/O facility",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "EXSyst"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "react/event-loop": ">=0.1 <0.5"
    },
    "suggest": {
        "react/event-loop": "Evented I/O : allows being called back when a stream has available data, instead of having to poll it yourself"
    },
    "autoload": {
        "psr-4": {
            "EXSyst\\Component\\IO\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
